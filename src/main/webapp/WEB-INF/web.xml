<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Auftueb</servlet-name>
        <servlet-class>rest.auftueb.Auftueb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Listbox</servlet-name>
        <servlet-class>rest.listbox.Listbox</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>rest.callux.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Auftueb</servlet-name>
        <url-pattern>/Auftueb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Listbox</servlet-name>
        <url-pattern>/Listbox</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
      30
    </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
