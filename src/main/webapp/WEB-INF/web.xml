        <servlet-class>rest.auftueb.Auftueb</servlet-class>
        <init-param>
            <param-name>status</param-name>
            <param-value>1</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>testnodb</servlet-name>
        <servlet-class>rest.test.testnodb</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>test</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Listbox</servlet-name>
        <servlet-class>rest.listbox.Listbox</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Listboxnodb</servlet-name>
        <servlet-class>restnodb.listbox.Listbox</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Auftuebnodb</servlet-name>
        <servlet-class>restnodb.auftueb.Auftueb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fetchexample</servlet-name>
        <servlet-class>rest.test.fetchexample</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>rest.callux.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Clientdata</servlet-name>
        <servlet-class>rest.callux.Clientdata</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sse</servlet-name>
        <servlet-class>rest.callux.Sse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Longpolling</servlet-name>
        <servlet-class>rest.callux.Longpolling</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Auftueb</servlet-name>
        <url-pattern>/Auftueb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testnodb</servlet-name>
        <url-pattern>/testnodb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Listbox</servlet-name>
        <url-pattern>/Listbox</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Listboxnodb</servlet-name>
        <url-pattern>/Listboxnodb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Auftuebnodb</servlet-name>
        <url-pattern>/Auftuebnodb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fetchexample</servlet-name>
        <url-pattern>/fetchexample</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Clientdata</servlet-name>
        <url-pattern>/Clientdata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sse</servlet-name>
        <url-pattern>/sse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Longpolling</servlet-name>
        <url-pattern>/Longpolling</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>webrw.html</welcome-file>
    </welcome-file-list>
</web-app>
